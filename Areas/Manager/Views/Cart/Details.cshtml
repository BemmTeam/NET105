
@model NET105.Entities.Cart

@{
    ViewData["Title"] = "Chi tiết giỏ hàng";
}


<div>
    <a class="btn btn-primary col-3" asp-action="Index"><i class="far fa-caret-square-left"></i> Quay lại</a>
    <hr>
      <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CreatedDate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CreatedDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Completed)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Completed)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Address)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Address)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Status)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Status)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CartDetail_Json)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CartDetail_Json)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.User)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.User.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Payment)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Payment.Name)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-warning text-light" asp-action="Edit" asp-route-id="@Model.CartId"><i class="fas fa-edit"></i>
        Edit</a>
</div>
















 